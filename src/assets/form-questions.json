{
    "form": {
        "timeBeforeVisibleInSeconds": 2,
        "id": "generalsurvey",
        "title": "General Questionnaire",
        "welcome": {
            "description": "We would like to ask you a few questions in order to get to know our readers more! It will only take a minute and we will not bother you again!"
        },
        "pages": [
            [
                {
                    "id": "name",
                    "label": "Name",
                    "type": "input"
                },
                {
                    "id": "email",
                    "label": "Email",
                    "type": "input"
                }
            ],
            [
                {
                    "id": "age",
                    "label": "Choose your age",
                    "required": true,
                    "type": "select",
                    "values": {
                        "from": 16,
                        "to": 100
                    }
                },
                {
                    "id": "gender",
                    "label": "Gender",
                    "required": true,
                    "type": "radio",
                    "values": [
                        ["male", "Male"],
                        ["female", "Female"],
                        ["nonbinary", "Non-Binary"],
                        ["donotdisclose", "Do Not Disclose"]
                    ]
                }
            ],
            [
                {
                    "id": "favouritebook",
                    "label": "Favourite Book",
                    "type": "input",
                    "required": true
                },
                {
                    "id": "favouritecolor",
                    "label": "Favourite Colors",
                    "required": true,
                    "type": "checkbox",
                    "values": [
                        ["black", "Black"],
                        ["white", "White"],
                        ["gray", "Gray"],
                        ["silver", "Silver"],
                        ["maroon", "Maroon"],
                        ["red", "Red"],
                        ["purple", "Purple"],
                        ["fushsia", "Fushsia"],
                        ["green", "Green"],
                        ["lime", "Lime"],
                        ["olive", "Olive"],
                        ["yellow", "Yellow"],
                        ["navy", "Navy"],
                        ["blue", "Blue"],
                        ["teal", "Teal"],
                        ["aqua", "Aqua"]
                    ],
                    "options": {
                        "includeOtherInput": false
                    }
                }
            ]
        ]
    }
}
